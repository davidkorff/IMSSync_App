# Application Settings
API_V1_STR="/api/v1"
PROJECT_NAME="IMS Integration API"
LOG_LEVEL=INFO
DEFAULT_ENVIRONMENT=iscmga_test

# API Security
API_KEYS=["test_api_key_1", "test_api_key_2"]
TRITON_API_KEYS=["triton_key_1", "triton_key_2"]
TRITON_CLIENT_IDS=["triton"]

# IMS One Environment
IMS_ONE_USERNAME=your_username
IMS_ONE_PASSWORD=encrypted_password

# ISCMGA Test Environment
ISCMGA_TEST_USERNAME=your_username
ISCMGA_TEST_PASSWORD=encrypted_password

# Triton Integration Settings
TRITON_DEFAULT_PRODUCER_GUID=00000000-0000-0000-0000-000000000000
TRITON_PRIMARY_LINE_GUID=00000000-0000-0000-0000-000000000000
TRITON_EXCESS_LINE_GUID=00000000-0000-0000-0000-000000000000
TRITON_PRIMARY_RATER_ID=1
TRITON_PRIMARY_FACTOR_SET_GUID=00000000-0000-0000-0000-000000000000
TRITON_EXCESS_RATER_ID=2
TRITON_EXCESS_FACTOR_SET_GUID=00000000-0000-0000-0000-000000000000

# Xuber Integration Settings
XUBER_DEFAULT_PRODUCER_GUID=00000000-0000-0000-0000-000000000000
XUBER_DEFAULT_LINE_GUID=00000000-0000-0000-0000-000000000000
XUBER_PRIMARY_RATER_ID=3
XUBER_PRIMARY_FACTOR_SET_GUID=00000000-0000-0000-0000-000000000000

# MySQL Database Settings (for Triton polling)
# For dev environment with SSM tunnel:
TRITON_MYSQL_HOST=localhost
TRITON_MYSQL_PORT=13307
TRITON_MYSQL_DATABASE=triton_dev
TRITON_MYSQL_USER=your_mysql_user
TRITON_MYSQL_PASSWORD=your_mysql_password

# Local MySQL Settings (for IMS transaction logs)
LOCAL_MYSQL_HOST=localhost
LOCAL_MYSQL_PORT=3306
LOCAL_MYSQL_DATABASE=ims_integration
LOCAL_MYSQL_USER=your_local_mysql_user
LOCAL_MYSQL_PASSWORD=your_local_mysql_password

# Polling Service Settings
POLL_INTERVAL_SECONDS=60
POLL_BATCH_SIZE=10
ENABLE_TRITON_POLLING=true

# SSM Tunnel Settings (if needed)
SSM_INSTANCE_ID=i-xxxxxxxxxxxxxxxxx
USE_SSM_TUNNEL=false

# CORS Settings (IMPORTANT: Change from ["*"] for production)
CORS_ORIGINS=["*"]